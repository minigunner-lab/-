local MarketplaceService = game:GetService("MarketplaceService")
getgenv().gameName = MarketplaceService:GetProductInfo(game.PlaceId).Name
local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
local Window = Rayfield:CreateWindow({
   Name = "Volt Hub - " .. getgenv().gameName,
   Icon = 0,
   LoadingTitle = "Lead the way, never follow.",
   LoadingSubtitle = "You are a sigma. I believe!",
   Theme = "Default",

   ToggleUIKeybind = "K",

   DisableRayfieldPrompts = false,
   DisableBuildWarnings = false, 

   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
      FileName = "VoltUniversalScriptFree"
   },

   Discord = {
      Enabled = true,
      Invite = "8HH3TacCa6", 
      RememberJoins = false
   },

   KeySystem = false,
})
Rayfield:Notify({
   Title = "Volt Hub",
   Content = "Loaded...",
   Duration = 6.5,
   Image = 4483362458,
})
